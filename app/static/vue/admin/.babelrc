{
  "env": {
    "development": {
      "presets": [
        ["env", { "modules": false }]
      ]
    },
    "test": {
      "presets": [
        ["env", {
          "modules": false,
          "targets": { "node": "current" }
        }]
      ],
      "plugins": ["istanbul", "transform-es2015-modules-commonjs"]
    }
  }
}
