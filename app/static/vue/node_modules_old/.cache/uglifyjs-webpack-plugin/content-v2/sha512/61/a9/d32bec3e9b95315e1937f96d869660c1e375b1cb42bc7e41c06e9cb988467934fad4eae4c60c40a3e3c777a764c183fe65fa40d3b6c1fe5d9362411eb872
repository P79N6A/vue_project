      }).then((response) => {\\r\\n                    this.reviewData = response[\\\"data\\\"][\\\"result\\\"][\\\"query_result\\\"];\\r\\n                    this.showCrud = true;\\r\\n                    bus.$emit(\\\"resetValues\\\");\\r\\n                }).catch((error) => {\\r\\n                    bus.$emit(\\\"showWarning\\\", error.response.data);\\r\\n                }).finally(() => {\\r\\n                    bus.$emit(\\\"hideLoading\\\");\\r\\n                }); \\r\\n            } else {\\r\\n                this.reviewData = [];\\r\\n                this.showCrud = true;\\r\\n                bus.$emit(\\\"resetValues\\\");\\r\\n            }\\r\\n        });\\r\\n        bus.$on(\\\"hideCrud\\\", () => {\\r\\n            this.showCrud = false;\\r\\n        });\\r\\n    }\\r\\n}\\r\\n</script>\\r\\n\\r\\n<style lang=\\\"scss\\\">\\r\\n\\r\\n* {\\r\\n    box-sizing: border-box;\\r\\n    font-family: 'Martel Sans', sans-serif;\\r\\n    font-weight: 200;\\r\\n}\\r\\n\\r\\n.app-view {\\r\\n    display: flex;\\r\\n}\\r\\n\\r\\n.main-view {\\r\\n    height: calc(100vh - 36px);\\r\\n    width: 100%;\\r\\n}\\r\\n\\r\\n</style>\\r\\n\",\"var render = function() {\\n  var _vm = this\\n  var _h = _vm.$createElement\\n  var _c = _vm._self._c || _h\\n  return _c(\\n    \\\"div\\\",\\n    [\\n      _c(\\\"top-nav\\\"),\\n      _vm._v(\\\" \\\"),\\n      _c(\\n        \\\"div\\\",\\n        { staticClass: \\\"app-view\\\" },\\n        [\\n          _c(\\\"side-nav\\\"),\\n          _vm._v(\\\" \\\"),\\n          _c(\\n            \\\"div\\\",\\n            { staticClass: \\\"main-view\\\" },\\n            [\\n              _c(\\n                \\\"div\\\",\\n                {\\n                  directives: [\\n                    {\\n                      name: \\\"show\\\",\\n                      rawName: \\\"v-show\\\",\\n                      value: _vm.isCommonView === true,\\n                      expression: \\\"isCommonView === true\\\"\\n                